{
  "expo": {
    "name": "Nutrithy",
    "slug": "nutrithy-app",
    "scheme": "nutrithy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "splash": {
      "image": "./src/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B132B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nutrithy.app"
    },
    "android": {
      "package": "com.nutrithy.app",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#0B132B"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "07c902a6-a1ea-402c-9c47-485bab11d345"
      }
    }
  }
}
